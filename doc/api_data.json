[  {    "type": "put",    "url": "/sensor/:id",    "title": "Modifies sensor status",    "name": "ModifySensor",    "group": "Sensors",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "id",            "description": "<p>The sensor ID</p>"          }        ],        "body": [          {            "group": "body",            "type": "Boolean",            "optional": false,            "field": "is_allocatable",            "description": "<p>The new allocatable status. For the moment it only accepts falsey values, it means that a client can only disable sensors.</p>"          }        ]      }    },    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "Object",            "optional": false,            "field": "sensor",            "description": "<p>The resulting Sensor entity</p>"          }        ]      },      "examples": [        {          "title": "Success-Response",          "content": "HTTP/1.1 200 OK\n{\n  \"sensor\": {\n    \"id\": \"abc1234\",\n    \"is_allocatable\": false\n  }\n}",          "type": "json"        }      ]    },    "error": {      "fields": {        "Error 4xx": [          {            "group": "Error 4xx",            "optional": false,            "field": "SensorNotFound",            "description": "<p>The <code>id</code> of the Sensor was not found.</p>"          },          {            "group": "Error 4xx",            "optional": false,            "field": "SensorAlreadyDisabled",            "description": "<p>The Sensor is already disabled.</p>"          },          {            "group": "Error 4xx",            "optional": false,            "field": "InvalidFields",            "description": "<p>The field <code>is_allocatable</code> has an invalid value or has not been provided</p>"          }        ]      },      "examples": [        {          "title": "Error-Response",          "content": "HTTP/1.1 404 Not Found\n{\n  \"error\": \"Not found\"\n}",          "type": "json"        },        {          "title": "Error-Response",          "content": "HTTP/1.1 400 Bad Request\n{\n  \"error\": \"The sensor is already disabled\"\n}",          "type": "json"        },        {          "title": "Error-Response",          "content": "HTTP/1.1 400 Bad Request\n{\n  \"error\": \"`is_allocatable` is required and should be falsey\"\n}",          "type": "json"        }      ]    },    "version": "0.0.0",    "filename": "src/routes/sensors.js",    "groupTitle": "Sensors"  },  {    "type": "post",    "url": "/workout/:id/allocations/participant",    "title": "Adds a new participant to the workout",    "description": "<p>It allocates him a sensor</p>",    "name": "AddParticipant",    "group": "Workouts",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "id",            "description": "<p>The workout ID</p>"          }        ],        "body": [          {            "group": "body",            "type": "String",            "optional": false,            "field": "user_id",            "description": "<p>The user ID</p>"          }        ]      }    },    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "Object",            "optional": false,            "field": "workout",            "description": "<p>The resulting workout with the new allocation</p>"          }        ]      },      "examples": [        {          "title": "Success-Response",          "content": "HTTP/1.1 200 OK\n{\n  \"workout\": {\n    \"id\": \"123\",\n    \"allocations\": [\n      {\n        \"user_id\": \"pm